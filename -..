def get_name():
    name = input("What is your name? ")
    return name

def hi(name_var):
    if name_var == "lauren" or name_var == "Lauren":
        print("hi lauren")
    elif name_var == "christian" or name_var =="Christian":
        print("hi christian. you're ok")
    else:
        print("bitch this ain't your computer!")

def rndomfx():
    for x in range(0,100,5):
        print(x)
    
hi(get_name())
    
rndomfx()
